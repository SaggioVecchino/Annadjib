        <Storyboard x:Key="OnLoaded1">
            <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.Opacity)" Storyboard.TargetName="buttonCours3">
                <EasingDoubleKeyFrame KeyTime="0" Value="0"/>
