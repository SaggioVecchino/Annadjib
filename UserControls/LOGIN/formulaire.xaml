        </StackPanel>
        <TextBox x:Name="textBoxps1" Height="35" TextWrapping="Wrap" Text="ادخل كلمة المرور" BorderThickness="2" GotFocus="textBoxps1_GotFocus" Background="White" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" VerticalAlignment="Center" Width="150" HorizontalAlignment="Center" FontSize="13" Foreground="#2196F3" BorderBrush="#2196F3" Margin="120,115,0,0"/>
